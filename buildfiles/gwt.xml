<?xml version="1.0" encoding="UTF-8"?>
<project name="gwt" basedir="../.">


    <target name="gwt" depends="gradle-gwt" description="Compiles all gwt targets"></target>

    <target name="gradle-gwt" description="Compiles all gwt targets">
        <!-- Delegate to Gradle for proper classpath handling -->
        <exec executable="gradle" failonerror="true">
            <arg value="gwtCompile"/>
        </exec>
    </target>

</project>
