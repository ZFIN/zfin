<!--
	Web application for Phenote on @MUTANT@
-->


<Context docBase="@TARGETROOT@/j2ee/phenote/deploy"
         privileged="false" antiResourceLocking="false" antiJARLocking="false" swallowOutput="true">

<Valve className="org.apache.catalina.valves.AccessLogValve"
                 fileDateFormat="yyyy-MM"
                 directory="logs"  prefix="@DOMAIN-NAME@_phenote_access_log." suffix=".txt"
                 pattern="common" resolveHosts="false"/>
 
 <Parameter name="lazy-load" value="true" override="true"/>

</Context>
