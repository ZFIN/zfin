<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="zfinPropertiesLoader" class="org.zfin.properties.presentation.ZfinTestPropertiesPlaceholderConfigurer">
        <property name="searchSystemEnvironment" value="false"/>
        <property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_FALLBACK"/>
        <!--<property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_OVERRIDE"/>-->
    </bean>

    <bean name="handlerAdapterf" class="org.springframework.web.servlet.mvc.annotation.AnnotationMethodHandlerAdapter"/>


</beans>