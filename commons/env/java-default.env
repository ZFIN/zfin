# To be included by a specific environment variable that the following:
# MUTANT_NAME

# deployment
setenv CATALINA_HOME /private/apps/tomcat
setenv CATALINA_BASE /private/etc/tomcat/$MUTANT_NAME
setenv CATALINA_PID "$CATALINA_BASE/catalina_pid"
