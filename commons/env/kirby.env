#!/local/bin/tcsh

setenv ANT_OPTS "-XX:PermSize=32m -XX:MaxPermSize=128m"
setenv CATALINA_BASE "/private/etc/tomcat/kirby"
setenv CATALINA_HOME "/private/apps/tomcat"
setenv CATALINA_PID "/private/etc/tomcat/kirby/catalina_pid"
setenv CURATION_DBNAME "false"
setenv CURATION_INSTANCE "false"
setenv DBNAME "kirbdb"
setenv DOMAIN_NAME "kirby.zfin.org"
setenv DOWNLOAD_DIRECTORY "/research/zunloads/download-files/kirbdb"
setenv HAS_PARTNER "false"
setenv INDEXER_UNLOAD_DIRECTORY "/research/zunloads/indexes/kirbdb"
setenv INDEXER_WIKI_HOSTNAME "devwiki.zfin.org"
setenv INDEXER_WIKI_PASSWORD "dan1orer1o"
setenv INDEXER_WIKI_USERNAME "webservice"
setenv INFORMIXDIR "/private/apps/Informix/informix"
setenv INFORMIXSERVER "waldo"
setenv INFORMIXSQLHOSTS "/private/apps/Informix/informix/etc/sqlhosts"
setenv INFORMIX_PORT "2002"
setenv INSTANCE "kirby"
setenv JAVA_HOME "/private/apps/java"
setenv JPDA_ADDRESS "9311"
setenv LD_LIBRARY_PATH "/private/apps/Informix/informix/lib:/private/apps/Informix/informix/lib/esql"
setenv MUTANT_NAME "kirby"
setenv ONCONFIG "onconfig"
setenv PARTNER_DBNAME "false"
setenv PARTNER_INTERNAL_INSTANCE "false"
setenv PARTNER_SOURCEROOT "false"
setenv SHARED_DOMAIN_NAME "false"
setenv SOURCEROOT "/research/zusers/pm/kirby/ZFIN_WWW"
setenv SQLHOSTS_HOST "zygotix.cs.uoregon.edu"
setenv TARGETCGIBIN "cgi-bin"
setenv TARGETFTPROOT "/research/zcentral/ftp/test/kirby"
setenv TARGETROOT "/research/zcentral/www_homes/kirby"
setenv USER "pm"


# Define SVNROOT
if("${INFORMIXSERVER}" == "wildtype") then
    if ("`/local/bin/hostname`" != "helix") then
      echo "WARNING: Not currently logged in to helix."
    endif
    setenv SVNROOT file:///research/zcentral/Vault/SVNROOT ;
    if (! $?PREINFORMIXPATH) then
        setenv PREINFORMIXPATH ${PATH}
    endif

    # now that commons exists on helix, include it as well.
    setenv PATH /private/bin:${INFORMIXDIR}/bin:/private/ZfinLinks/Commons/bin:${PREINFORMIXPATH}
    if ($?PREINFORMIX_LD_PATH) then
        setenv LD_LIBRARY_PATH ${PREINFORMIX_LD_PATH}:${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    else
        setenv LD_LIBRARY_PATH ${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    endif
else if("${INFORMIXSERVER}" == "waldo") then
    if ("`/local/bin/hostname`" != "zygotix" ) then
      echo "WARNING: Not currently logged in to zygotix."
    endif
    # Set PATH variables and prompt using the standard variables
    # First check if PREINFORMIXPATH IS SET
    if (! $?PREINFORMIXPATH) then
        setenv PREINFORMIXPATH ${PATH}
    endif
    setenv PATH /private/bin:${INFORMIXDIR}/bin:/private/ZfinLinks/Commons/bin:${PREINFORMIXPATH}
    if ($?PREINFORMIX_LD_PATH) then
        setenv LD_LIBRARY_PATH ${PREINFORMIX_LD_PATH}:${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    else
        setenv LD_LIBRARY_PATH ${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    endif
    setenv SVNROOT svn+ssh://${USER}@zygotix.cs.uoregon.edu/research/zcentral/Vault/SVNROOT
else if("${INFORMIXSERVER}" == "whitebread") then
    if ("`/local/bin/hostname`" != "kinetix" ) then
      echo "WARNING: Not currently logged in to kinetix."
    endif
    # Set PATH variables and prompt using the standard variables
    # First check if PREINFORMIXPATH IS SET
    if (! $?PREINFORMIXPATH) then
        setenv PREINFORMIXPATH ${PATH}
    endif
    setenv PATH /private/bin:${INFORMIXDIR}/bin:/private/ZfinLinks/Commons/bin:${PREINFORMIXPATH}
    if ($?PREINFORMIX_LD_PATH) then
        setenv LD_LIBRARY_PATH ${PREINFORMIX_LD_PATH}:${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    else
        setenv LD_LIBRARY_PATH ${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    endif
    setenv SVNROOT svn+ssh://${USER}@zygotix.cs.uoregon.edu/research/zcentral/Vault/SVNROOT
else
    if ("`/local/bin/hostname`" != "embryonix" ) then
      echo "WARNING: Not currently logged in to embryonix."
    endif
    # Set PATH variables and prompt using the standard variables
    # First check if PREINFORMIXPATH IS SET
    if (! $?PREINFORMIXPATH) then
        setenv PREINFORMIXPATH ${PATH}
    endif
    setenv PATH /private/bin:${INFORMIXDIR}/bin:/private/ZfinLinks/Commons/bin:${PREINFORMIXPATH}
    if ($?PREINFORMIX_LD_PATH) then
        setenv LD_LIBRARY_PATH ${PREINFORMIX_LD_PATH}:${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    else
        setenv LD_LIBRARY_PATH ${INFORMIXDIR}/lib:${INFORMIXDIR}/lib/esql
    endif
    setenv SVNROOT svn+ssh://${USER}@zygotix.cs.uoregon.edu/research/zcentral/Vault/SVNROOT
endif


# Prompt
test ${?prompt} -eq 1 && set prompt = "${MUTANT_NAME} %c2$Prompt"

if ($user == 'informix') then
  umask 022
endif

