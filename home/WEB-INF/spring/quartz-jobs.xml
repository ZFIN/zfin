                        <entry key="jobName" value="Load Sequence Ontology"/>
                    </map>
                </property>
            </bean>
        </property>
        <property name="misfireInstructionName" value="MISFIRE_INSTRUCTION_SMART_POLICY"/>
        <!--goes off every day at 4.10 am-->
        <property name="cronExpression" value="5 0 17 ? * MON-FRI"/>
    </bean>
    <!--NOTE: these are both autowired in the zfinapp context-->
    <!--<bean id="downloadService1" class="org.zfin.datatransfer.service.DownloadService"/>-->
    <!--<bean id="gafService1" class="org.zfin.datatransfer.go.service.GafService"/>-->
