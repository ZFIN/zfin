                    </map>
                </property>
            </bean>
        </property>
        <property name="misfireInstructionName" value="MISFIRE_INSTRUCTION_SMART_POLICY"/>
        <!--goes off every day at 4.10 am-->
        <property name="cronExpression" value="4 0 17 ? * MON-FRI"/>
    </bean>

     <bean id="loadBehaviorOntology" class="org.springframework.scheduling.quartz.CronTriggerBean">
        <property name="jobDetail">
            <bean class="org.springframework.scheduling.quartz.JobDetailBean">
                <property name="name" value="Behavior Ontology"/>
                <property name="group" value="Load Ontology"/>
                <property name="jobClass" value="org.zfin.ontology.quartz.LoadOntologiesJob"/>
                <property name="jobDataAsMap">
                    <map>
                        <entry key="oboFileName" value="behavior.obo"/>
                        <entry key="oboFileUrl"
                               value="http://behavior-ontology.googlecode.com/svn/trunk/behavior.obo"/>
                        <entry key="scriptDirectory" value="../server_apps/data_transfer/LoadOntology"/>
                        <entry key="scriptFileName" value="loadTerms.sql,handleSecondaryTerms.sql,handleRelationships.sql,handleSynonyms.sql,fixAnnotationsUponOntologyLoad.sql,loadSubsets.sql,updateDefinitionReferences.sql,dropTempTables.sql"/>
                        <entry key="useExistingFile" value="false"/>
                        <entry key="jobName" value="Load Behavior Ontology"/>
                    </map>
                </property>
            </bean>
        </property>
        <property name="misfireInstructionName" value="MISFIRE_INSTRUCTION_SMART_POLICY"/>
        <!--goes off every day at 4.10 am-->
        <property name="cronExpression" value="6 0 17 ? * MON-FRI"/>
    </bean>

     <bean id="loadSequenceOntology" class="org.springframework.scheduling.quartz.CronTriggerBean">
        <property name="jobDetail">
            <bean class="org.springframework.scheduling.quartz.JobDetailBean">
                <property name="name" value="Sequence Ontology"/>
                <property name="group" value="Load Ontology"/>
                <property name="jobClass" value="org.zfin.ontology.quartz.LoadOntologiesJob"/>
                <property name="jobDataAsMap">
                    <map>
                        <entry key="oboFileName" value="so.obo"/>
                        <entry key="oboFileUrl"
                               value="http://obo.cvs.sourceforge.net/viewvc/obo/obo/ontology/genomic-proteomic/so.obo"/>
                        <entry key="scriptDirectory" value="../server_apps/data_transfer/LoadOntology"/>
                        <entry key="scriptFileName" value="loadTerms.sql,handleSecondaryTerms.sql,handleRelationships.sql,handleSynonyms.sql,fixAnnotationsUponOntologyLoad.sql,loadSubsets.sql,dropTempTables.sql"/>
