<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
        "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
        
<tiles-definitions>


    <definition name="phenotype/*.page" extends="main_layout">
        <put-attribute name="body" value="/WEB-INF/jsp/phenotype/{1}.jsp"/>
    </definition>

    <definition name="phenotype/*.insert" template="/WEB-INF/jsp/phenotype/{1}.jsp"/>

    <definition name="phenotype/*.popup" extends="popup_layout">
        <put-attribute name="body" value="/WEB-INF/jsp/phenotype/{1}.jsp"/>
    </definition>
    
    <definition name="fish/*.popup" extends="popup_layout">
        <put-attribute name="body" value="/WEB-INF/jsp/fish/{1}.jsp"/>
    </definition>

    <definition name="genotype/*.popup" extends="popup_layout">
        <put-attribute name="body" value="/WEB-INF/jsp/feature/{1}.jsp"/>
    </definition>


</tiles-definitions>
