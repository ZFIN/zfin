

<html>
<body>
<?MIVAR NAME=$pubZdbID>$pubID<?/MIVAR>
<?MIVAR NAME=$externallink>http://www.ncbi.nlm.nih.gov:80/entrez/query.fcgi?cmd=search&db=PubMed&dopt=Abstract&term=$accession_no<?/MIVAR>
<?MIVAR NAME=$sourcelink>$referenceurl<?/MIVAR>

<?MICOMMENT> ** START ELSEVIER STATS ** <?/MICOMMENT>
<?MIVAR>


<?MICOMMENT> 
$pubZdbID
$externallink
$sourcelink
<?/MICOMMENT> 

<?MISQL SQL="
    insert into elsevier_statistics 
    ( es_incoming_ip, es_zfin_url, es_figure_zdb_id,  es_external_link, es_date ,es_http_user_agent)
    values
    ( '$REMOTE_ADDR','$sourcelink' , '$pubZdbID',  '$externallink' , CURRENT,'$HTTP_USER_AGENT') 
    ;">
<?/MISQL>
<?MICOMMENT> ** END ELSEVIER STATS ** <?/MICOMMENT>

<?MIVAR NAME="redirecttime">0<?/MIVAR>

Redirecting to <a href="$externallink">$externallink</a>.

<meta http-equiv="refresh" content="$redirecttime;url=$externallink">

<?/MIVAR>

<?MICOMMENT> 
<br/> <br/>
<h2>LOG</h2>
<br/> <br/>
<table border=1>
    <tr>
    <th>incoming_ip </th>
    <th>url </th>
    <th>zdb_id </th>
    <th>externallink </th>
    <th>time</th>
    </tr>
<?MISQL SQL="
    select es_incoming_ip, es_zfin_url, es_figure_zdb_id,  es_external_link, es_date  from elsevier_statistics  ; 
    ">
    <tr>
    <td>$1 </td>
    <td>$2 </td>
    <td>$3 </td>
    <td>$4 </td>
    <td>$5 </td>
    </tr>
<?MICOMMENT> 
   <?MIVAR NAME=$incoming_ip>$1<?/MIVAR>
   <?MIVAR NAME=$url>$2<?/MIVAR>
   <?MIVAR NAME=$zdb_id>$3<?/MIVAR>
   <?MIVAR NAME=$external_link>$4<?/MIVAR>
   <?MIVAR NAME=$date>$5<?/MIVAR>
<?/MICOMMENT> 
<?/MISQL>
</table>

<?/MICOMMENT> 







</body>

</html>


    
