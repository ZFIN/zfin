         ;">
        <option value="$1"
        $(IF,$(AND,$(XST,$featcur_feature_type_only),$(EC,$featcur_feature_type_only,$1)),SELECTED)>$2</option>
      <?/MISQL>
    </select>
  </td>
</table>

