<HTML>
<head>
<title>Submit a Proposed Locus/Line Name</title>

<SCRIPT>

function isBlank(s)
{
  var len=s.length
  if(len==0)
    return true

  var i
  for(i=0;i<len;i++)
  {
    if(s.charAt(i)!=" ") return false
  }

  return true
}

function valid(fieldname,fieldvalue)
{
  if(isBlank(fieldvalue))
  {
    alert(fieldname+" cannot be left blank.")
    return false
  }
  return true
}
function validateForm()
{
  if(!valid("Submitter Name",document.input.submitter_name.value))
    return false

  if(!valid("Email",document.input.email.value))
    return false

  if(!valid("Laboratory",document.input.lab_name.value))
    return false

  return true;
}

function clearPubStatus() {
	document.getElementById('pub_info_ispub').style.display="none";
	document.getElementById('pub_info_nopub').style.display="none";
}

</SCRIPT>

</head>

