<!-- This file is almost identical to the file used to generate printable pubs, except that it generated a mail list that can be passed to sendmail.

-->

<PRE>
<?MISQL SQL="
select distinct email 
from person p 
join address a on a.add_pk_id=p.pers_address
where on_dist_list='t' 
and upper(a.add_country_code) like '%USA%' 
and email is not null 
and email <> ''
order by email asc
;
">$1
<?/MISQL>
</PRE>
