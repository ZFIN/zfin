<!-- Cause a tile for the new page to appear in the navigator frame -->
<!-- startframes.apg will pass variables on to appropriate page -->
<!-- don't want a tile if got here via the accessionlister  -->


<?MIBLOCK COND="$(AND,$(NXST,$accession),$(NC,$line_type,wild_type))">
  <?MIVAR NAME=page_title>Find MUTANT<?/MIVAR>
  <?MIVAR name=$TileInput>$TileInput&navtile=true<?/MIVAR>
  <?MIVAR name=$TileInput COND="$(XST,$BEGIN)">$TileInput&BEGIN=$BEGIN<?/MIVAR>
  <?MIVAR name=$TileInput COND="$(XST,$WINSIZE)">$TileInput&WINSIZE=$WINSIZE<?/MIVAR>
 
  <?MIVAR COND="$(XST,$(URLENCODE,$fname))" NAME=$(URLENCODE,$fname)><?/MIVAR>
  <?MIVAR COND="$(XST,$(URLENCODE,$anon1text))" NAME=$(URLENCODE,$anon1text)><?/MIVAR> 
  <?MIVAR COND="$(NXST,$fname)" NAME=$fname><?/MIVAR>
  <?MIVAR COND="$(NXST,$anon1text)" NAME=$anon1text><?/MIVAR>

  <!-- replaced top.content.location.href with hardcoding of location.  href contained variables after the navtile button was used, This could cause some variables to be used that were not in the current selection.  -->

  <SCRIPT>
    <?MIBLOCK COND="$(XST,$structures)">
      <?MIVAR>
        var page_id='/<!--|WEBDRIVER_PATH_FROM_ROOT|-->?MIval=aa-startframes.apg&frame_size=250&selector=aa-fishselect.apg&select_from=Mutants&line_type=mutant&intro=/ZFIN/help_files/mut_intro.html$TileInput&structures=$structures&fname=$(URLENCODE,$fname)&anon1text=$(URLENCODE,$anon1text)';
      <?/MIVAR>
    <?MIELSE>
       <?MIVAR>
         var page_id='/<!--|WEBDRIVER_PATH_FROM_ROOT|-->?MIval=aa-startframes.apg&frame_size=250&selector=aa-fishselect.apg&select_from=Mutants&line_type=mutant&intro=/ZFIN/help_files/mut_intro.html$TileInput&fname=$(URLENCODE,$fname)&anon1text=$(URLENCODE,$anon1text)';
      <?/MIVAR>
    <?/MIBLOCK>

    <?MIVAR>
      top.update_hist(page_id,"$page_title");
      top.controls.location.reload(true);
    <?/MIVAR>

  </SCRIPT>
<?/MIBLOCK>
