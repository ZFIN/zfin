     <?MICOMMENT>------------------------------------------------------------
               -- temp table $fishselect_temp_on_anat_junior holds geno zdb id
               -- and anatomy zdb id pairs 
               ------------------------------------------------------------
     <?/MICOMMENT>

     <?MIVAR NAME=$fishselect_temp_on_anat_junior>$(CONCAT,fishselect_temp_on_anat_junior_,$fsel_session_id)<?/MIVAR>    
     <?MISQL SQL="
        execute function table_exists('$fishselect_temp_on_anat_junior')">
        <?MISQL COND=$(EC,$1,f) SQL="
           create temp table $fishselect_temp_on_anat_junior (
             junior_g_zdb_id 		varchar(50),
             junior_anat_zdb_id         varchar(50)
            ) with NO LOG;">
        <?/MISQL>
     <?/MISQL>

     <?MICOMMENT> -------------------------------------------------------------
             -- get genotypes directly related to the anatomy terms
             ---------------------------------------------------------------------
     <?/MICOMMENT>
     <?MIBLOCK COND="$(EC,$fselFilterValue,moExcluded)">
       <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, gffs_superterm_zdb_id
	       from genotype_figure_fast_search 
              where gffs_morph_zdb_id is null
                and gffs_superterm_zdb_id in ($anatIDs);">
       <?/MISQL>
     
       <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, gffs_subterm_zdb_id
	       from genotype_figure_fast_search 
              where gffs_morph_zdb_id is null
                and gffs_subterm_zdb_id in ($anatIDs);">
       <?/MISQL>    
     <?MIELSE>  
       <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, gffs_superterm_zdb_id
	       from genotype_figure_fast_search 
              where gffs_superterm_zdb_id in ($anatIDs);">
       <?/MISQL>
     
       <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, gffs_subterm_zdb_id
	       from genotype_figure_fast_search 
              where gffs_subterm_zdb_id in ($anatIDs);">
       <?/MISQL>        
     <?/MIBLOCK>

     <?MICOMMENT> -------------------------------------------------------------
             -- if include substructure
             ---------------------------------------------------------------------
     <?/MICOMMENT>   
     <?MIBLOCK COND="$(AND,$(XST,$include_substructures),$(NE,$include_substructures,),$(NE,$include_substructures,unchecked))">  
       <?MIBLOCK COND="$(EC,$fselFilterValue,moExcluded)">
         <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, allanatcon_container_zdb_id
	       from genotype_figure_fast_search, all_anatomy_contains
              where gffs_morph_zdb_id is null
                and gffs_superterm_zdb_id = allanatcon_contained_zdb_id
                and allanatcon_container_zdb_id in ($anatIDs);">
         <?/MISQL>
       
         <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, allanatcon_container_zdb_id
	       from genotype_figure_fast_search, all_anatomy_contains
              where gffs_morph_zdb_id is null
                and gffs_subterm_zdb_id = allanatcon_contained_zdb_id
                and allanatcon_container_zdb_id in ($anatIDs);">
         <?/MISQL>    
         
      <?MIELSE>
         <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, allanatcon_container_zdb_id
	       from genotype_figure_fast_search, all_anatomy_contains
              where gffs_superterm_zdb_id = allanatcon_contained_zdb_id
                and allanatcon_container_zdb_id in ($anatIDs);">
         <?/MISQL>
       
         <?MISQL SQL="
	insert into $fishselect_temp_on_anat_junior(junior_g_zdb_id, junior_anat_zdb_id)
	     select gffs_geno_zdb_id, allanatcon_container_zdb_id
	       from genotype_figure_fast_search, all_anatomy_contains
              where gffs_subterm_zdb_id = allanatcon_contained_zdb_id
                and allanatcon_container_zdb_id in ($anatIDs);">
         <?/MISQL> 
      <?/MIBLOCK>
    
    <?/MIBLOCK>  <?MICOMMENT>-- end of if include substructures --<?/MICOMMENT>
    
    <?MICOMMENT> -------------------------------------------------------------
             -- "and" search vs. "or" search 
             ---------------------------------------------------------------------
    <?/MICOMMENT>  
    <?MIBLOCK COND="$(AND,$(XST,$structure_bool),$(EC,$structure_bool,and))">

       <?MISQL SQL="  
     	 insert into $fishselect_temp_on_anat (g_zdb_id)
              select junior_g_zdb_id
                from $fishselect_temp_on_anat_junior
            group by junior_g_zdb_id
              having count (distinct junior_anat_zdb_id) = $item_count;">
       <?/MISQL>
    <?MIELSE>
       <?MISQL SQL="  
     	 insert into $fishselect_temp_on_anat (g_zdb_id)
              select distinct junior_g_zdb_id  
                from $fishselect_temp_on_anat_junior;">
       <?/MISQL>   
    <?/MIBLOCK>

    <?MICOMMENT> ------- Clean up temp table---------------<?/MICOMMENT> 
    <?MISQL SQL="delete from $fishselect_temp_on_anat_junior;"><?/MISQL>

    <?MIBLOCK COND="$(>,$item_count,0)">
    
      <?MISQL SQL="select distinct g_zdb_id from $fishselect_temp_on_anat;">
        <?MIVAR NAME=fsel_anat_geno>$1<?/MIVAR>
        <?MISQL SQL="select geno_is_wildtype from genotype where geno_zdb_id = '$fsel_anat_geno';">
          <?MIVAR NAME=fsel_anat_isWildtype>$1<?/MIVAR>
        <?/MISQL>    
    
        <?MIVAR NAME=call_wildtypePhenoAnat>f<?/MIVAR>
        
        <?MICOMMENT> *** if it's not a wildtype, look up to see if it's a Tg *** <?/MICOMMENT>
        <?MIBLOCK COND="$(EC,$fsel_anat_isWildtype,f)"> 
          <?MISQL SQL="select genofeat_zdb_id
                         from genotype_feature, feature_marker_relationship, marker_relationship EFG
                        where genofeat_geno_zdb_id = '$fsel_anat_geno'
                          and genofeat_feature_zdb_id = fmrel_ftr_zdb_id
                          and fmrel_type != 'contains phenotypic sequence feature'
                          and fmrel_mrkr_zdb_id = EFG.mrel_mrkr_1_zdb_id
                          and EFG.mrel_mrkr_2_zdb_id like 'ZDB-EFG%'
                          and not exists(select 'x' 
                                           from genotype_feature, feature_marker_relationship
                                          where genofeat_geno_zdb_id = '$fsel_anat_geno'
                                            and genofeat_feature_zdb_id = fmrel_ftr_zdb_id
                                            and fmrel_type = 'is allele of');">
            <?MIVAR>$(SETVAR,$call_wildtypePhenoAnat,t)<?/MIVAR>
          <?/MISQL>
        <?MIELSE> <?MICOMMENT> *** must call wildtypePhenoAnat.apg if it's wildtype *** <?/MICOMMENT>
          <?MIVAR>$(SETVAR,$call_wildtypePhenoAnat,t)<?/MIVAR>
        <?/MIBLOCK>

        <?MIVAR NAME=fsel_anat_fig_count>1<?/MIVAR>
        <?MICOMMENT> *** call wildtypePhenoAnat.apg to get the figure count for the wildtype or Tg *** <?/MICOMMENT>
        <?MIBLOCK  COND="$(EC,$call_wildtypePhenoAnat,t)">
          <?MISQL SQL="select WebExplode(object,'wtphn_calling_page=fishselectAnat&wtphn_session_id=$fsel_session_id&wtphn_geno_zdb_id=$fsel_anat_geno&wtphn_processed_selected_structures=$fsel_processed_selected_structures&wtphn_include_substructures=$include_substructures&wtphn_structure_bool=$structure_bool') 
                         from webPages where ID='aa-wildtypePhenoAnat.apg';">$1<?/MISQL>
          <?MIVAR>$(SETVAR,$fsel_anat_fig_count,$wtphn_figCt)<?/MIVAR>
        <?/MIBLOCK>
        
        <?MICOMMENT> *** only insert the genotype id to the temp table when the figure count is greater than 0 *** <?/MICOMMENT>
        <?MISQL COND="$(>,$fsel_anat_fig_count,0)" SQL="
                     insert into $fishselect_temp_on_anatfig (g_zdb_id)
                          values ('$fsel_anat_geno');">
        <?/MISQL>        
      <?/MISQL>
      <?MIVAR NAME=fishselect_temp_on_anat_table>$fishselect_temp_on_anatfig<?/MIVAR>
    
    <?MIELSE>
      <?MIVAR NAME=fishselect_temp_on_anat_table>$fishselect_temp_on_anat<?/MIVAR>
    <?/MIBLOCK>

    <?MICOMMENT> ------- Define constraints for final SQL ---------------<?/MICOMMENT> 
    <?MIBLOCK  COND="$(XST,$moreConstraint)">
      <?MIVAR NAME=$moreConstraint> and exists (select 'x' from $fishselect_temp_on_anat_table where geno_zdb_id = g_zdb_id) <?/MIVAR>
    <?MIELSE>
      <?MIVAR NAME=$moreConstraint> where exists (select 'x' from $fishselect_temp_on_anat_table where geno_zdb_id = g_zdb_id) <?/MIVAR> 
    <?/MIBLOCK>

    <?MIVAR>$(SETVAR,$constraint,$(CONCAT,$constraint,$moreConstraint))<?/MIVAR>

