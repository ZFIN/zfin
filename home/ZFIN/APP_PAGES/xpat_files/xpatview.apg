<?MIVAR NAME=$COUNT>0<?/MIVAR>    
<?MIVAR NAME=$TEMP_FIMG_PREP> <?/MIVAR>

  <?MISQL SQL="
    SELECT fimg_direction, fimg_zdb_id, fimg_thumbnail, fimg_view, fimg_preparation
