                  $5<br>&nbsp;
                </td>
              <?/MIVAR>
            <?MIELSE>
              <?MIVAR>
                <td align=left valign=top>
                  $5<br>$4, $1
                </td>
              <?/MIVAR>
            <?/MIBLOCK>
              </tr>
            </table>
          </td>
      <?/MIVAR>
      <?MIVAR NAME=$COUNT>3<?/MIVAR>
    <?/MIBLOCK>

    <?MIBLOCK COND="$(=,$COUNT,2)">
      <?MIVAR>
          <td width=50%>
            <table border=0 cellpadding=2>
              <tr>
                <td>
