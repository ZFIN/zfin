    window.location.replace('/<!--|WEBDRIVER_PATH_FROM_ROOT|-->?MIval=aa-xpatupdate.apg');
    }

    function updateStageWindow(xpatstg_xpat,xpatstg_start,xpatstg_end) {
    window.location.replace('/<!--|WEBDRIVER_PATH_FROM_ROOT|-->?MIval=aa-xpatupdate.apg');
    }

    function updateImage(xpatstg_xpat,xpatstg_start,xpatstg_end) {
    window.location.replace('?MIval=aa-xpatupdate.apg');
    }

