<TITLE>xpatselect: failed searches</TITLE>


<?MISQL SQL="select WebExplode(object,'') from webPages where ID='aa-htmlpageheader.apg';">$1<?/MISQL>

  <H2>xpatselect: failed searches</H2>

  <?MIBLOCK COND="$(XST,$delete_term)">
    <?MISQL SQL="delete from xpat_anatomy_capture where xac_anatitem_name = '$delete_term'">
      <font color=red>All instances of '$delete_term' removed</font>
    <?/MISQL>
  
  <?/MIBLOCK>

  <HR width="80%" align=left>  



  <TABLE>
  <?MISQL SQL="SELECT xac_anatitem_name, xac_date FROM xpat_anatomy_capture ORDER BY xac_date;">
    <?MIVAR NAME="xac_anatitem_name">$1<?/MIVAR>
    <?MIVAR NAME="xac_date">$2<?/MIVAR>
    <?MIVAR>

    <TR>
      <TD bgcolor=#EEEEEE>
        $xac_anatitem_name <BR>
      </TD>
      <TD>
        <FORM method="get" action="/<!--|WEBDRIVER_PATH_FROM_ROOT|-->">
	  <INPUT type="hidden" name="MIval" value="aa-anatomy_captures.apg">
	  <INPUT type="hidden" name="delete_term" value="$xac_anatitem_name">
	  <INPUT type="submit" value="delete">
	</FORM>
     </TD>
    </TR>
    <?/MIVAR>
  <?/MISQL>

<?MISQL SQL="select WebExplode(object,'') from webPages where ID='aa-htmlpagefooter.apg';">$1<?/MISQL>
