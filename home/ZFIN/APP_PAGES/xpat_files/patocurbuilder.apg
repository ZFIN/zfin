        <td> <input type="radio" name="$patobldr_api_zdb_id" id="$(CONCAT,$patobldr_radio_id,add)"    value="add"
             onChange="if (this.checked == true) { patoCuration.distinctPhenotypes['$patobldr_radio_id'].setState('add');
                       deselectDefault(); }"> </td>
        <td onclick="rotateRadio('$patobldr_radio_id');">
            <?MIVAR>
             <a id="$(CONCAT,patobldr_api_zdb_id,_b$MI_CURRENTROW)" href="/phenote/Phenote/?termId=$patobldr_superterm_obo_id&ontologyName=$patobldr_superterm_ontology&field=ENTITY&viewType=EXTERNAL" onmouseover=addcontrol("$(CONCAT,patobldr_api_zdb_id,_b$MI_CURRENTROW)")>$patobldr_superterm_name_html</a>
