<script type="text/javascript">
  function setEntityOntology(selectedOntology, autoCompleter) {
    if (selectedOntology == 'AO') {
      ontologyName = 'ZF';
    } else if (selectedOntology == 'GO') {
      ontologyName = 'GO';
    }

    autoCompleter.options.defaultParams = "ontologyName=" + ontologyName;


  }

</script>
