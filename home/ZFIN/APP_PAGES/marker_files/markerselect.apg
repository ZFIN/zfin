             <?MICOMMENT>== when MOs continue on the same gene ==<?/MICOMMENT>
             <?MIELSE>
                 <?MIVAR NAME=$display_target_gene> &nbsp; <?/MIVAR>

             <?/MIBLOCK> <?MICOMMENT> end of line display difference <?/MICOMMENT>

             <?MIVAR NAME=$target_gene_zdb_id>$2<?/MIVAR>

        <?/MISQL>
      <?/MIBLOCK> 

