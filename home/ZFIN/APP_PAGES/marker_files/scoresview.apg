<HTML>

<HEAD>
<SCRIPT>
<?MIVAR>
function redisplay () {     
top.helpcontent.location="/cgi-bin/webdriver?MIval=aa-showscores.apg&panel=$panel_id&lg=$OR_LG&marker_id=$marker&marker_abbrev=$marker_abbrev";

}
<?/MIVAR>
</SCRIPT>
</HEAD>

<?MIBLOCK COND="$(NC,$panel_id,NULL)">
<?MISQL SQL="select abbrev from panels where zdb_id='$panel_id';">
<?/MISQL>
<?MIVAR NAME=$panel_abbrev>$1<?/MIVAR>
<?/MIBLOCK>

<?MIVAR NAME=$highlight><!--|HIGHLIGHT_COLOR|--><?/MIVAR>
<?MIVAR><BODY bgcolor="$highlight"><?/MIVAR>

<form name=lgselect method=post>

<?MIVAR>
<TABLE width=100%>
<TR>
<TD align=center><b><font size=3>$panel_abbrev Panel;
$(IF,$(NC,$OR_LG,None),LG $OR_LG)
$(IF,$(AND,$(NC,$marker_abbrev,undefined),$(NC,$marker_abbrev,None)),; $marker_abbrev)
 </B>
</TD>
<TD align=right><font size=2><input type=button value="Close" onClick="parent.close();"> </TD>
</TR>
</TABLE>
<?/MIVAR>


</form>

<script>redisplay();   </script>

</HTML>


