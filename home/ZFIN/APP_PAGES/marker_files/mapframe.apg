<html>



  <head>
<?MIVAR>    <title>MapViewer $map_panel - $lg</title> <?/MIVAR>
  </head>

  <body>

<?MIBLOCK COND="$(NXST,$lg)">Error - No Linkgage Group Specified!<?/MIBLOCK>

<?MIBLOCK COND="$(EQ,$version,beta)">
<applet code="MapViewerApplet" codebase="/client_apps/mapper/"  height=400 width=600 name="MapViewer1" archive="mapper.jar" mayscript>
<?/MIBLOCK>

<?MIBLOCK COND="$(EQ,$version,release)">
<applet code="MapViewerApplet" codebase="/client_apps/mapper/release/"  height=400 width=600 name="MapViewer1" archive="mapper.jar" mayscript>
<?/MIBLOCK>


<?MIBLOCK COND="$(XST,$query)">
<param name="query" value = "query">
<?/MIBLOCK>

<?MIBLOCK COND="$(XST,$selected_marker)">
<?MIVAR><param name="selected_marker"  value="$selected_marker"> <?/MIVAR>
<?/MIBLOCK>

<?MIBLOCK COND="$(XST,$map_panel)">
<?MIVAR><param name="map_panel" value = "$map_panel"> <?/MIVAR>
<?/MIBLOCK>


<?MIVAR><param name="LG" value= "$lg"><?/MIVAR>
<param name="marker_url" value="/action/marker/view/">
<param name="ApplicationClass" value="bioapps.MapViewer">
</applet>

Hold down <B>SHIFT</B> to find marker distances.  <i>(NOTE: Applet may take a few moments to load)</i> 


  </body>
</html>
