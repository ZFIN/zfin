<?MIVAR NAME=$count>0<?/MIVAR>
<?MIVAR NAME=$clr_cnt>0<?/MIVAR>
<?MIVAR NAME=$line_clr>#e5e5e5<?/MIVAR>

<?MISQL SQL="select zdb_id,name,panel_date,ptype,abbrev from panels where status='incomplete';">

$(SETVAR,$clr_cnt,$(+,$clr_cnt,1))

