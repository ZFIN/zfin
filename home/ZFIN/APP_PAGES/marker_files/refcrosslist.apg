$(SETVAR,$line_clr,$(IF,$(!=,$color,$(FIX,$color)),#FCCE64,#e5e5e5))

<TR bgcolor=$line_clr><TD><font size=3><A HREF=/cgi-bin_B/webdriver?MIval=aa-crossview.apg&OID=$1>$2 ($5)</A></TD><TD><font size=2><b>Type:</B> $4</TD></TR>

<?/MISQL>

</TABLE>

