      <command>cd $TARGETROOT/server_apps/data_transfer/RNACentral &amp;&amp; ./LoadRNACentralIDs.groovy -jobName Load-RNACentralLinks_m</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
