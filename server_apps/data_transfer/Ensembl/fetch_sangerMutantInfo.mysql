select t.stable_id, g.stable_id, tl.stable_id, x.display_label
from gene g 
join transcript t on g.gene_id = t.gene_id
left join translation tl on tl.transcript_id = t.transcript_id 
left join xref x on g.display_xref_id = x.xref_id 
join external_db ed using (external_db_id) order by t.stable_id;

 