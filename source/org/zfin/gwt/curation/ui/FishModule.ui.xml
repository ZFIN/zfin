<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:zfin='urn:import:org.zfin.gwt.root.ui'
        xmlns:zfin-mod='urn:import:org.zfin.gwt.curation.ui'
        >

    <g:VerticalPanel spacing="10" width="100%">
        <zfin-mod:GenotypeView ui:field="genotypeView"/>
        <zfin-mod:ImportGenotype ui:field="importGenotypeView"/>
        <zfin-mod:GenotypeConstruction ui:field="genotypeConstructionView"/>
        <zfin-mod:FishConstruction ui:field="fishConstructionView"/>
        <zfin-mod:FishView ui:field="fishView"/>
    </g:VerticalPanel>
</ui:UiBinder>
