        xmlns:zfin-fish='urn:import:org.zfin.gwt.curation.ui.fish'
        >

    <g:FlowPanel>
        <zfin-fish:FishGenotypeConstruction ui:field="genotypeConstructionView" styleName="mb-3"/>
